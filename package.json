{
  "name": "gridsome-blog-fix",
  "private": true,
  "scripts": {
    "prebuild": "npm run build:markdown",
    "build": "gridsome build",
    "build:markdown": "tafsiri -d blog",
    "build:staging": "run-s build noindex",
    "dev": "gridsome develop",
    "new": "hygen post new",
    "noindex": "echo 'User-agent: *\nDisallow: /' > dist/robots.txt",
    "prestart": "npm run build:markdown",
    "start": "run-p watch:markdown dev",
    "watch:markdown": "tafsiri -d blog -w"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.5",
    "@gridsome/source-filesystem": "^0.6.2",
    "@gridsome/transformer-remark": "^0.6.2",
    "gridsome": "^0.7.20",
    "gridsome-plugin-pug": "0.0.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@croccifixio/tafsiri": "^1.1.10",
    "hygen": "^5.0.3",
    "marked": "^0.7.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.4",
    "sass-loader": "^8.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2"
  }
}
