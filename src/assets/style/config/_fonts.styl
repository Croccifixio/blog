@font-face {
	font-family: 'Cartograph CF';
  font-style: normal;
  font-weight: normal;
	src: url("../fonts/CartographCF-Light.woff2") format("woff2");
}

@font-face {
	font-family: 'Cartograph CF';
	font-style: italic;
  font-weight: bold;
	src: url("../fonts/CartographCF-BoldItalic.woff2") format("woff2");
}

sans-serif-fallback = -apple-system, BlinkMacSystemFont, "Segoe UI", "Roboto", "Oxygen", "Ubuntu", "Cantarell", "Fira Sans", "Droid Sans", "Helvetica Neue", sans-serif

body-font() {
  font-feature-settings 'ss05'
}
body-font-bold-italic() {
  font-feature-settings 'ss01', 'ss05'
  font-style italic
  font-weight bold
}
body-font-bold-italic-fancy() {
  font-feature-settings 'ss05'
  font-style italic
  font-weight bold
}
code-font() {
  font-feature-settings 'ss03'
  font-variant-ligatures common-ligatures
}
code-font-bold-italic() {
  font-feature-settings 'ss01'
  font-style italic
  font-variant-ligatures common-ligatures
  font-weight bold
}
